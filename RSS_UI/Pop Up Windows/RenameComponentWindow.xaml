        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RSS_UI.Pop_Up_Windows"
        mc:Ignorable="d"
        Title="Rename Channel/Feed" Height="72.875" Width="437.958">
    <Grid>
        <Label Content="New Name:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox Name="RenameTextBox" HorizontalAlignment="Left" Height="23" Margin="87,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="246" KeyDown="RenameTextBox_Enter"/>
        <Button Name="RenameButton" Content="Rename" HorizontalAlignment="Left" Margin="345,13,0,0" VerticalAlignment="Top" Width="75" Click="RenameButton_Click"/>

    </Grid>
</Window>
